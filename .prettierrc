{
	"useTabs": false,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 120,
	"tabWidth": 4,
	"semi": true,
	"bracketSpacing": true,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}