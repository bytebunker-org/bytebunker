{
    "name": "@bytebunker/backend",
    "version": "0.0.1",
    "description": "",
    "author": {
        "name": "Moritz Hein",
        "email": "moritz.hein@live.de",
        "url": "https://moritz.website"
    },
    "private": true,
    "type": "module",
    "license": "MIT",
    "scripts": {
        "build": "nest build",
        "build:generate-metadata": "tsx --tsconfig tsconfig.lib.json ./tools/scripts/generate-metadata.ts",
        "build:lib": "tsx --tsconfig tsconfig.lib.json ./tools/scripts/build-lib.script.ts",
        "dev": "cross-env NODE_ENV=development TZ=UTC nest start --watch",
        "mikro-orm": "mikro-orm-esm",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,tools,test}/**/*.ts\" --fix",
        "start": "cross-env NODE_ENV=production TZ=UTC nest start",
        "start:debug": "cross-env NODE_ENV=development TZ=UTC nest start --debug --watch",
        "start:prod": "cross-env NODE_ENV=production TZ=UTC node dist/main.js",
        "test": "vitest run",
        "test:cov": "vitest run --coverage",
        "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
        "test:e2e": "vitest run --config ./vitest-e2e.config.ts",
        "test:watch": "vitest",
        "test:watch-ui": "vitest --ui",
        "nest": "nest"
    },
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "types": "./lib/index.d.ts"
        }
    },
    "main": "./dist/main.js",
    "typesVersions": {
        "*": {
            ".": [
                "./lib/index.d.ts"
            ]
        }
    },
    "files": [
        "./lib"
    ],
    "engines": {
        "node": ">=22",
        "pnpm": ">=9"
    },
    "dependencies": {
        "@bytebunker/qs-esm": "workspace:*",
        "@mikro-orm/core": "^6.4.2",
        "@mikro-orm/nestjs": "^6.0.2",
        "@mikro-orm/postgresql": "^6.4.2",
        "@nestjs/axios": "^3.1.3",
        "@nestjs/cache-manager": "^2.3.0",
        "@nestjs/cli": "^10.4.9",
        "@nestjs/common": "^10.4.15",
        "@nestjs/core": "^10.4.15",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.4.15",
        "@nestjs/platform-fastify": "^10.4.15",
        "@nestjs/schedule": "^4.1.2",
        "@nestjs/swagger": "^8.1.0",
        "@nestjs/terminus": "^10.2.3",
        "ajv": "^8.17.1",
        "axios": "^1.7.9",
        "bcrypt": "^5.1.1",
        "cache-manager": "^5.7.6",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "express": "^4.21.2",
        "express-basic-auth": "^1.2.1",
        "express-session": "^1.18.1",
        "helmet": "^8.0.0",
        "js-convert-case": "^4.2.0",
        "json-schema": "^0.4.0",
        "luxon": "^3.5.0",
        "morgan": "^1.10.0",
        "nest-typed-config": "^2.9.3",
        "passport": "^0.7.0",
        "passport-custom": "^1.1.1",
        "passport-local": "^1.0.0",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "svelte-advanced-datatable": "^0.6.3",
        "ts-decorator-json-schema-generator": "^1.4.1",
        "tsx": "4.19.2"
    },
    "devDependencies": {
        "@bytebunker/eslint-config": "workspace:*",
        "@bytebunker/tsconfig": "workspace:*",
        "@eslint/js": "^9.17.0",
        "@lewo-media/nestjs-schematics": "^10.2.3",
        "@mikro-orm/cli": "^6.4.2",
        "@mikro-orm/reflection": "^6.4.2",
        "@nestjs/schematics": "^10.2.3",
        "@nestjs/testing": "^10.4.15",
        "@suites/di.nestjs": "^3.0.1",
        "@suites/doubles.vitest": "^3.0.1",
        "@suites/types.doubles": "^3.0.0",
        "@suites/unit": "^3.0.1",
        "@swc/cli": "^0.5.2",
        "@swc/core": "^1.10.4",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.1",
        "@types/json-schema": "^7.0.15",
        "@types/luxon": "^3.4.2",
        "@types/morgan": "^1.9.9",
        "@types/node": "22.9.0",
        "@types/passport-local": "^1.0.38",
        "@types/pg": "^8.11.10",
        "@types/supertest": "^6.0.2",
        "@vitest/coverage-v8": "^1.6.0",
        "@vitest/ui": "^1.6.0",
        "cross-env": "^7.0.3",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-n": "^17.15.1",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-unicorn": "^56.0.1",
        "prettier": "^3.4.2",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.19.0",
        "unplugin-swc": "^1.5.1",
        "vitest": "^2.1.8",
        "vitest-mock-extended": "^2.0.2"
    },
    "lint-staged": {
        "*.{js,ts}": [
            "eslint --fix",
            "prettier --write"
        ]
    }
}
